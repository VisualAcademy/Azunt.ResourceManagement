{
    //[9] DB 초기화 여부
    "Database": {
        "InitializeOnStartup": true, // 앱 시작 시 DB 초기화 여부
        "ResourceSeeder": {
            "Enable": true, // 시드 실행 여부
            "AppNames": [] // 빈 배열이면 전체 실행 (= appName:null)
        }
    },

    "ConnectionStrings": {
        "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=AzuntResource;Trusted_Connection=True;MultipleActiveResultSets=true"
    },


    //[5] 애플리케이션 이름 설정
    "AppName": "ReportWriter", // 애플리케이션 이름


    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}
